num_A=[7,5,10,2,3,9,7]
print(num_A)
 print(len(num_A))
 print(num_A.index(7))
num_B=[9,10,3,4,2,5,7,1]
 print(num_B)
 print(len(num_B))
 print(num_B.index(7)) 
num_A.append(1) 
 print(Array_A)
num_B.append(14)
 print(num_B)
 num_C = num_A.copy() 
 print(Array_C) 
 Array_C.extend(num_B) 
 print(num_C) 
 print(num_C.count(7))
 num_C.sort()
num_C.remove(7)
print(num_C)
num_D = num_C.copy()
print(num_D) 
num_D.reverse() 
print(num_D)
